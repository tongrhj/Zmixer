$compose-colour= #EF49B7

.Compose-Panel
  position absolute
  top 0
  z-index 0
  background-color black
  height calc(100vh - 152px)
  width 100vw
  padding 8px
  display flex
  flex-direction column
  overflow scroll

.Compose-TagList
  display flex
  flex-direction column
  justify-content space-around

.Compose-Panel-hidden
  display none
